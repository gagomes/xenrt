<xenrt>

  <!-- MNR VM clock stability test sequence -->

  <variables>
    <PRODUCT_VERSION>Boston</PRODUCT_VERSION>
    <PREPARE_WORKERS>2</PREPARE_WORKERS>
  </variables>

  <prepare>
    <host>
      <vm name="ws08-x86">
        <distro>ws08-x86</distro>
        <memory>512</memory>
        <vcpus>2</vcpus>
        <network device="0"/>
        <postinstall action="installDrivers"/>
      </vm>
      <vm name="ws08-x64">
        <distro>ws08-x64</distro>
        <memory>512</memory>
        <vcpus>2</vcpus>
        <network device="0"/>
        <postinstall action="installDrivers"/>
      </vm>
      <vm name="ws08r2-x64">
        <distro>ws08r2-x64</distro>
        <memory>512</memory>
        <vcpus>2</vcpus>
        <network device="0"/>
        <postinstall action="installDrivers"/>
      </vm>
    </host>
  </prepare>

  <testsequence group="VMClockStability">
    <parallel>
      <testcase id="xenserver.tc.vmlifecycle.TC8259" prio="3"/>
      <testcase id="xenserver.tc.vmlifecycle.TC8260" prio="3"/>
      <testcase id="xenserver.tc.vmlifecycle.TC9716" prio="3"/>
    </parallel>
  </testsequence>

</xenrt>
