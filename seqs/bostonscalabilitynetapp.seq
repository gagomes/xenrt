<xenrt>

  <!-- Boston scalability on NetApp test sequence -->

  <variables>
    <PRODUCT_VERSION>Boston</PRODUCT_VERSION>
    <INSTALL_SR_TYPE>ext</INSTALL_SR_TYPE>
  </variables>

  <prepare>
    <host>
      <storage type="netapp" name="netappsr" options="thin"/>
    </host>
  </prepare>

  <testsequence group="NetApp">
    <testcase id="xenserver.tc.scalability.TC8086"/>
  </testsequence>

</xenrt>
