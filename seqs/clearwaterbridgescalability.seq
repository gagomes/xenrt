  <testsequence>
    <serial group="vSwitch">
      <testcase id="xenserver.tc.vswitch.TC11544bridge" tc="TC-11544" />
    </serial>
    <serial>
      <finally>
        <testcase id="guestops.basic.TCShutdown">
