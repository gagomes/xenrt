        </vm>
      </host>
      <host id="4"> 
        <vm name="linux_4">
          <distro>generic-linux</distro>
        </vm>
      </host>
      <host id="5">
        <vm name="linux_5">
          <distro>generic-linux</distro>
        </vm>
      </host>
      <host id="6">
        <vm name="linux_6">
          <distro>generic-linux</distro>
        </vm>
      </host>
      <host id="7">
