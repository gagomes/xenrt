<xenrt>
<!-- Pool recovery from power failure - gathering data for performance team-->

  <variables>
    <PRODUCT_VERSION>Clearwater</PRODUCT_VERSION>
    <INSTALL_SR_TYPE>lvm</INSTALL_SR_TYPE>
  </variables>

  <prepare>
    <pool>
      <allhosts/>
    </pool>
  </prepare>

  <testsequence>
    <serial group="Functional Tests">
      <testcase id="testcases.xenserver.tc.host.TCPowerFailureRecovery" tc="TC-19084"/>
   </serial>
  </testsequence>
</xenrt>
