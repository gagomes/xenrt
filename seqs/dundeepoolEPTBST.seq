<xenrt>

  <!-- Pool based EPT tests -->

  <variables>
    <PRODUCT_VERSION>Dundee</PRODUCT_VERSION>
    <PREPARE_WORKERS>2</PREPARE_WORKERS>
  </variables>

  <prepare>
    <pool>
      <storage type="nfs" name="nfssr" default="true"/>
      <allhosts/>
    </pool>
  </prepare>

  <testsequence>
    <parallel group="EPT">
      <testcase id="xenserver.tc.vmlifecycle.TCwin2k8x86"/>
      <testcase id="xenserver.tc.vmlifecycle.TCwin2k8x64"/>
    </parallel>
  </testsequence>

</xenrt>
