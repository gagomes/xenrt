    </serial>

    <!-- centos64 with * -->
    <serial group="centos64-centos64">
      <testcase id="xenserver.tc.perf.tc_networkthroughput2.TCNetworkThroughputPointToPoint">
        <arg>threads=1</arg>
        <arg>endpoint0=centos64-0</arg>
	<arg>endpoint1=centos64-1</arg>
      </testcase>
    </serial>
    <serial group="centos64-rhel64">
      <testcase id="xenserver.tc.perf.tc_networkthroughput2.TCNetworkThroughputPointToPoint">
