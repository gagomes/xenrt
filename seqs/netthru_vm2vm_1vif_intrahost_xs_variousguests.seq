    <!-- rhel64 with * -->
    <serial group="rhel64-rhel64">
      <testcase id="xenserver.tc.perf.tc_networkthroughput2.TCNetworkThroughputPointToPoint">
        <arg>threads=1</arg>
        <arg>endpoint0=rhel64-0</arg>
	<arg>endpoint1=rhel64-1</arg>
      </testcase>
    </serial>

  </testsequence>
</xenrt>
