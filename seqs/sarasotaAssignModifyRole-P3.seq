                       <arg>10435</arg>
               </testcase>
               <testcase id="testcases.xenserver.tc.gui.TestComplete" tc="TC-10436" name="TC-10436">
                       <arg>http://hg.uk.xensource.com/closed/guitest/sarasota.hg</arg>
                       <arg>TC_10436</arg>
                       <arg>TC10436</arg>
                       <arg>10436</arg>
               </testcase>
