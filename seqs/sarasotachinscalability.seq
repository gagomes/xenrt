    </pool>
  </prepare>

  <testsequence>
    <serial group="vSwitch">
      <testcase id="xenserver.tc.vswitch.TC12543"/>
      <testcase id="xenserver.tc.vswitch.TC12551"/>
    </serial>
    <serial>
      <finally>
        <testcase id="guestops.basic.TCShutdown">
            <arg>guest=controller</arg>
            <arg>again</arg>
        </testcase>
      </finally>
    </serial>
  </testsequence>

