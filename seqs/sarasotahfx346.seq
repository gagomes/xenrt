<xenrt>
  <variables>
    <PRODUCT_VERSION>Sarasota</PRODUCT_VERSION>
  </variables>

  <prepare>
    <host id="0">
      <vm name="linux">
        <distro>debian60</distro>
        <memory>128</memory>
        <vcpus>2</vcpus>
        <network device="0" />
        <disk device="0" size="8" />
      </vm>
    </host>
  </prepare>

  <testsequence>
    <serial group="storage">
      <testcase id="xenserver.tc.storage.TC17895" />
    </serial>
  </testsequence>
</xenrt>
