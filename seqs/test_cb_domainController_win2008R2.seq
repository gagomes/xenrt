<xenrt>
  <variables>
    <PRODUCT_VERSION>Creedence</PRODUCT_VERSION>
    <OPTION_DEFAULT_SR>yes</OPTION_DEFAULT_SR>
  </variables>

  <prepare>
    <host id="0">
      <vm name="ADController">
        <distro>ws08r2-x64</distro>
        <arch>x86-64</arch>
        <memory>2048</memory>
        <vcpus>2</vcpus>
        <postinstall action="setupDomainServer" />
        <network device="0" />
        <network device="1" />
        <disk device="0" size="32" />
      </vm>
      <vm name="Client1">
        <distro>ws08r2-x64</distro>
        <arch>x86-64</arch>
        <memory>2048</memory>
        <vcpus>2</vcpus>
        <network device="0" />
        <network device="1" />
        <disk device="0" size="24" />
      </vm>
      <vm name="Client2">
        <distro>ws08r2-x64</distro>
        <arch>x86-64</arch>
        <memory>2048</memory>
        <vcpus>2</vcpus>
        <network device="0" />
        <network device="1" />
        <disk device="0" size="24" />
      </vm>
    </host>
    <host id="1">
      <vm name="ws2012A">
        <distro>ws12r2-x64</distro>
        <arch>x86-64</arch>
        <memory>2048</memory>
        <vcpus>2</vcpus>
        <postinstall action="installDrivers" />
        <network device="0" />
        <disk device="0" size="40" />
      </vm>
      <vm name="ws2012B">
        <distro>ws12r2-x64</distro>
        <arch>x86-64</arch>
        <memory>2048</memory>
        <vcpus>2</vcpus>
        <postinstall action="installDrivers" />
        <network device="0" />
        <disk device="0" size="32" />
      </vm>
      <vm name="ws2003A">
        <distro>w2k3eesp2</distro>
        <memory>1024</memory>
        <vcpus>2</vcpus>
        <postinstall action="installDrivers" />
        <network device="0" />
        <disk device="0" size="32" />
      </vm>
      <vm name="ws2003A">
        <distro>w2k3eesp2</distro>
        <memory>1024</memory>
        <vcpus>2</vcpus>
        <postinstall action="installDrivers" />
        <network device="0" />
        <disk device="0" size="16" />
      </vm>
    </host>
  </prepare>

</xenrt>
