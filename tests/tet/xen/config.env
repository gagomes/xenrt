#
# TET/Local envrionment configuration
#

export TET_ROOT=/opt/tet
export TEMPOUT=/tmp
export TSROOT=$TET_ROOT/xen


export PATH=".:$TET_ROOT/bin:$PATH"

#XenOpt specific env variables

# Controller app path
export ctlr_app_path=/opt/xensource/xenman 
# Location of "xenman" binary
export ctlr_bin_loc=$ctlr_app_path/bin
# Controller binary name
export ctlr_bin_name="xenman"

#TET Test Case specific env variables
export expected_res_loc=$TSROOT/data


